{
  "output_folder": "reports/",
  "page_objects_path": [
    "test/e2e/pages"
  ],
  "selenium": {
    "start_process": true
  },
  "test_workers": false,
  "test_settings": {
    "default": {
      "source_folders": "test/e2e/tests",
      "skip_testcases_on_fail": false,
      "screenshots": {
        "enabled": true,
        "on_failure": true,
        "path": "screenshots/"
      }
    }
  }
}
